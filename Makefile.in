LIBPCAP = ../libpcap/libpcap-0.0.6/libpcap.a
LIBTCL = /usr/local/lib/libtcl74.a
INCLUDES	= -I/usr/local/include

CFLAGS	= -g ${INCLUDES}

all:	tcpd2det fix2det tehone

tcpd2det:	tcpd2det.o
	${CC} ${CFLAGS} -o tcpd2det ${LDFLAGS} tcpd2det.o ${LIBPCAP}

fix2det:	fix2det.o
	${CC} ${CFLAGS} -o fix2det ${LDFLAGS} fix2det.o

tehone:		tehone.o
	${CC} ${CFLAGS} -o tehone ${LDFLAGS} tehone.o ${LIBTCL} ${LIBPCAP} -lm
